@model MVC.Models.HeThongPhanPhoi

@{
    ViewData["Title"] = "Xóa hệ thống phân phối";
}

<h1>Xóa hệ thống phân phối</h1>

<h3>Bạn có chắc muốn xóa mục này?</h3>
<hr />
<dl class="row">
    <dt class="col-sm-2">Mã HTPP</dt>
    <dd class="col-sm-10">@Model.MaHTPP</dd>

    <dt class="col-sm-2">Tên HTPP</dt>
    <dd class="col-sm-10">@Model.TenHTPP</dd>
</dl>

<form asp-action="Delete">
    <input type="hidden" asp-for="MaHTPP" />
    <button type="submit" class="btn btn-danger">Xóa</button>
    <a asp-action="Index" class="btn btn-secondary">Hủy</a>
</form>

